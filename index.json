      "<style>",
      "  .orientation-warning {",
      "    display: none;",
      "    position: fixed;",
      "    top: 0;",
      "    left: 0;",
      "    width: 100%;",
      "    height: 100%;",
      "    background-color: #231F20;",
      "    color: white;",
      "    font-family: Arial, sans-serif;",
      "    text-align: center;",
      "    padding-top: 20%;",
      "    z-index: 9999;",
      "  }",
      "  @media (orientation: portrait) {",
      "    .orientation-warning { display: block; }",
      "    #unity-container { display: none !important; }",
      "  }",
      "</style>",
      "<script>",
      "  function checkOrientation() {",
      "    if (window.matchMedia('(orientation: portrait)').matches) {",
      "      document.querySelector('.orientation-warning').style.display = 'block';",
      "      if (document.getElementById('unity-container')) {",
      "        document.getElementById('unity-container').style.display = 'none';",
      "      }",
      "    } else {",
      "      document.querySelector('.orientation-warning').style.display = 'none';",
      "      if (document.getElementById('unity-container')) {",
      "        document.getElementById('unity-container').style.display = 'block';",
      "      }",
      "    }",
      "  }",
      "  window.addEventListener('load', checkOrientation);",
      "  window.addEventListener('resize', checkOrientation);",
      "  window.addEventListener('orientationchange', checkOrientation);",
      "</script>"
    ],
    "body": [
      "<div class='orientation-warning'>",
      "  <h1>Пожалуйста, переверните устройство</h1>",
      "  <p>Игра поддерживает только горизонтальную ориентацию.</p>",
      "</div>"
    ]
  },
